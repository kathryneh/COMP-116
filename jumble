D={}
for word in fp:
    word=word.strip()
    key=list(word)
    sort(key)
    key=''.join(key)
    #can't use a list as a key to a dictionary
    D[key] = word

    pickle : store stuff 

import cPickle as pickle
pickle.dump(D, file('jumbledict.pkl', 'w')
